{% comment %}
  SYNAPEX CLINICAL DIVISION LANDING PAGE
  Style: Brutalist, High Contrast, Industrial
  Version: 2.0 (Deployment Sequence + Tally Integrated)
{% endcomment %}

<style>
  /* BRUTALIST CSS RESET & STYLES */
  .sxs-b2b-wrapper {
    background-color: #000000;
    color: #FFFFFF;
    font-family: 'Inter', -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif;
    line-height: 1.4;
    padding-bottom: 80px;
    width: 100%;
    overflow-x: hidden;
  }
  
  .sxs-container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 24px;
  }

  /* UTILITIES */
  .sxs-accent { color: #E3DAC9; } /* Synapex Beige */
  .sxs-uppercase { text-transform: uppercase; }
  .sxs-bold { font-weight: 900; }

  /* HERO SECTION */
  .sxs-hero {
    padding: 140px 0 80px;
    border-bottom: 1px solid #333;
  }
  .sxs-eyebrow {
    color: #E3DAC9;
    font-size: 0.85rem;
    font-weight: 700;
    letter-spacing: 0.2em;
    text-transform: uppercase;
    margin-bottom: 24px;
    display: block;
  }
  .sxs-headline {
    font-size: clamp(2.5rem, 6vw, 5rem); /* Responsive font size */
    line-height: 0.95;
    font-weight: 900;
    text-transform: uppercase;
    margin-bottom: 30px;
    letter-spacing: -0.02em;
    color: #FFF;
  }
  .sxs-subhead {
    font-size: 1.25rem;
    color: #888;
    max-width: 650px;
    margin-bottom: 50px;
    font-weight: 400;
  }
  .sxs-btn {
    background-color: #E3DAC9;
    color: #000;
    padding: 20px 48px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    text-decoration: none;
    display: inline-block;
    transition: all 0.2s;
    border: none;
    cursor: pointer;
  }
  .sxs-btn:hover {
    background-color: #FFF;
    transform: translateY(-2px);
  }

  /* SPECS GRID */
  .sxs-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 60px;
    padding: 100px 0;
    border-bottom: 1px solid #333;
  }
  .sxs-card h3 {
    color: #E3DAC9;
    font-size: 1.25rem;
    font-weight: 700;
    text-transform: uppercase;
    margin-bottom: 15px;
    border-left: 4px solid #E3DAC9;
    padding-left: 20px;
    letter-spacing: 0.05em;
  }
  .sxs-card p {
    color: #CCC;
    line-height: 1.6;
    font-size: 1rem;
    padding-left: 24px;
  }

  /* ECONOMICS TABLE */
  .sxs-economics {
    padding: 100px 0;
    border-bottom: 1px solid #333;
  }
  .sxs-section-head {
    font-size: clamp(1.8rem, 4vw, 3rem);
    text-transform: uppercase;
    font-weight: 900;
    margin-bottom: 10px;
    color: #FFF;
  }
  .sxs-table-wrapper {
    border: 1px solid #333;
    margin-top: 50px;
    background: #050505;
  }
  .sxs-row {
    display: grid;
    grid-template-columns: 1fr 1fr 1fr;
    border-bottom: 1px solid #333;
  }
  .sxs-row:last-child { border-bottom: none; }
  .sxs-cell {
    padding: 24px;
    font-size: 1rem;
    display: flex;
    align-items: center;
  }
  .sxs-cell.head { font-weight: 700; color: #666; font-size: 0.8rem; letter-spacing: 0.1em; }
  .sxs-cell.highlight { color: #E3DAC9; font-weight: 700; background: rgba(227, 218, 201, 0.05); }
  .sxs-cell.center { justify-content: center; text-align: center; border-left: 1px solid #333; }
  
  /* PASSIVE INCOME FORMULA */
  .sxs-roi {
    padding: 100px 0;
    text-align: center;
    border-bottom: 1px solid #333;
    background: #050505;
  }
  .sxs-formula {
    font-size: clamp(1.5rem, 5vw, 4rem);
    font-weight: 900;
    text-transform: uppercase;
    color: #E3DAC9;
    letter-spacing: -0.02em;
  }
  .sxs-roi p { color: #888; margin-top: 10px; font-size: 1.1rem; }

  /* DEPLOYMENT SEQUENCE (PROCESS) */
  .sxs-process {
    padding: 100px 0;
    border-bottom: 1px solid #333;
  }
  .sxs-process-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 20px;
    margin-top: 50px;
  }
  .sxs-step {
    border: 1px solid #222;
    padding: 40px;
    background: #0A0A0A;
  }
  .sxs-step-num {
    color: #E3DAC9;
    font-size: 0.9rem;
    font-weight: 700;
    margin-bottom: 20px;
    display: block;
    letter-spacing: 0.1em;
  }
  .sxs-step h4 {
    color: #FFF;
    font-size: 1.5rem;
    font-weight: 900;
    text-transform: uppercase;
    margin-bottom: 10px;
  }
  .sxs-step p { color: #888; font-size: 0.95rem; }

  /* CTA / FORM SECTION */
  .sxs-cta-section {
    background-color: #080808;
    padding: 100px 0;
    text-align: center;
  }

  @media (max-width: 768px) {
    .sxs-row { font-size: 0.8rem; }
    .sxs-cell { padding: 15px; }
    .sxs-process-grid { grid-template-columns: 1fr; }
    .sxs-formula { font-size: 2rem; }
  }
</style>

