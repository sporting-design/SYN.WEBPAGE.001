              "block_order": [
                "text_qN7qXG"
              ]
            },
            "accordion_row_iYzhWR": {
              "type": "_accordion-row",
              "settings": {
                "heading": "How long can I wear SYNAPEX tape?",
                "open_by_default": false
              },
              "blocks": {
                "text_HDKyc6": {
                  "type": "text",
                  "settings": {
                    "text": "<p>You can safely wear SYNAPEX tape for 3–5 days straight. The German adhesive holds through showers, workouts, and even swimming. Simply remove when it starts to lose tension or whenever you’re done with your training cycle.</p>",
                    "width": "100%",
                    "max_width": "normal",
                    "alignment": "left",
                    "type_preset": "rte",
                    "font": "var(--font-body--family)",
