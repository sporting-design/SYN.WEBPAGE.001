            "accordion_row_tVM8Rr": {
              "type": "_accordion-row",
              "settings": {
                "heading": "How do I pick the right tape size or strip?",
                "open_by_default": false
              },
              "blocks": {
                "text_qN7qXG": {
                  "type": "text",
                  "settings": {
                    "text": "<p>Each zone on our body map links to a tailored product: pre-cut strips for common hotspots (neck, knee, shin, etc.) or full 5 cm × 5 m rolls if you need custom lengths. Tap your pain point on the map to see exactly which strip or roll suits your muscle or joint.</p>",
                    "width": "100%",
                    "max_width": "normal",
