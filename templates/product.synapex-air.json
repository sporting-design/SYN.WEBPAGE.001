              ]
            },
            "accordion_row_RJVeaa": {
              "type": "_accordion-row",
              "settings": {
                "heading": "Is SYNAPEX tape safe for sensitive skin?",
                "open_by_default": false
              },
              "blocks": {
                "text_RAJg4U": {
                  "type": "text",
                  "settings": {
                    "text": "<p>Absolutely. Our tape backing is latex-free and gentle on skin. For extra caution, test a small patch for 15 minutes before full application. If you have a known adhesive allergy, consult your physician before use.</p>",
                    "width": "100%",
                    "max_width": "normal",
